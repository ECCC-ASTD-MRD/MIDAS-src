<MODULE name="UnitTest">
  <SUBMITS sub_name="get"/>

  <TASK  name="get">
    <SUBMITS sub_name="run"/>
  </TASK>

  <TASK name="run">
    <SUBMITS sub_name="check"/>
  </TASK>

  <TASK name="check">
    <SUBMITS sub_name="clean"/>
  </TASK>

  <TASK name="clean">
    <SUBMITS sub_name="update"/>
  </TASK>

  <TASK name="update"/>
</MODULE>
