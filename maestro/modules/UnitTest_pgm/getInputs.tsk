#!/bin/ksh

input=$(readlink ${TASK_INPUT}/inputs.ca)
$SEQ_BIN/nodelogger -n $SEQ_NODE -s infox -m "Downloading input: ${input}"
if [ -f "${input}" ]; then
    ln -s ${input} inputs.ca
else
    ${TASK_BIN}/remote_copy ${input} inputs.ca
fi

${TASK_BIN}/cmcarc -f inputs.ca -x -v
rm inputs.ca
mv * ${TASK_OUTPUT}
