#!/bin/ksh
#.*****************************************************************************
#.
#.     JOB NAME - Manip
#.
#.     STATUS - OPERATIONAL - ESSENTIAL
#.
#.     DESCRIPTION - This job allows some level of customization by allowing the execution of a script
#.                   specified by the user (see 'ENVAR_manip_script').  
#.
#.*****************************************************************************

if [ -f ${TASK_BIN}/manip_script ]; then
    $SEQ_BIN/nodelogger -n $SEQ_NODE -s infox -m  "Executing $(true_path ${TASK_BIN}/manip_script)"
    . ${TASK_BIN}/manip_script
else
    $SEQ_BIN/nodelogger -n $SEQ_NODE -s infox -m  "Nothing done"
fi
