      subroutine fillmvo(cdmvo,cdvar,pvar,knlev,knobs)
#if defined (DOC)
*
***s/r fillmvo - Fill in COMMVOG and/or COMMVOHR
*
*
*     Author  : S. Pellerin ARMA/AES SEPT. 2000
*     Revision:
*           JM Belanger CMDA/SMC  Oct 2000
*                   . 32 bits conversion (Generic LOG)
*           C. Charette - ARMA/SMC - Sept 2004
*                   . Conversion to hybrid vertical coordinate
*
**    Purpose: Fill in COMMVOG/COMMVOHR with trial profiles
*
*Arguments
*
*       input:
*             CDMVO (character*2) : HR fills COMMVOHR
*                                   BG fills COMMVOG
*             CDVAR (character*2) : NOMVAR of the state variable
*             PVAR(knlev,knobs)   : Variable to transfer in COMMVO(G)(HR)
*             KNLEV (integer)     : number of levels of PVAR
*             KNOBS (integer)     : number of observation profiles in variable PVAR
*
#endif
      use columnData_mod 
 
      implicit none
*implicits
#include "pardim.cdk"
#include "comdim.cdk"
#include "comdimo.cdk"
#include "comgem.cdk"
#include "comphy.cdk"
#include "comcst.cdk"
c
      integer knlev,knobs
      real*8 pvar(knlev,knobs)
      character*2 cdvar,cdmvo
c
      integer jobs,jlev,inlev,istep
c
      if(cdmvo.eq.'HR') then
        inlev = columnhr%nlev
        istep = columnhr%nkgdimo
      elseif(cdmvo.eq.'BG') then
        inlev = columng%nlev
        istep = columng%nkgdimo
      endif
c
      if (cdvar.eq.'PP') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            do jlev = 1,inlev
              columnhr%rppobs(jlev,jobs) = pvar(jlev,jobs)
            enddo
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            do jlev = 1,inlev
              columng%rppobs(jlev,jobs) = pvar(jlev,jobs)
            enddo
          enddo
        endif
      elseif(cdvar.eq.'LV') then
        if(cdmvo.eq.'HR') then
          do jlev = 1,inlev
            columnhr%vlev(jlev) = pvar(jlev,1)
          enddo
        elseif(cdmvo.eq.'BG') then
          do jlev = 1,inlev
            vlev(jlev) = pvar(jlev,1)
          enddo
        endif
      elseif(cdvar.eq.'HY') then
        if(cdmvo.eq.'HR') then
          do jlev = 1,inlev
            columnhr%vhyb(jlev) = pvar(jlev,1)
          enddo
        elseif(cdmvo.eq.'BG') then
          do jlev = 1,inlev
            vhybinc(jlev) = pvar(jlev,1)
          enddo
        endif
      elseif(cdvar.eq.'UU') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columnhr%uu(jlev,jobs)=pvar(jlev,jobs)*rmsknt
            enddo
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columng%uu(jlev,jobs)=pvar(jlev,jobs)*rmsknt
            enddo
          enddo
        endif
      elseif(cdvar.eq.'VV') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columnhr%vv(jlev,jobs)=pvar(jlev,jobs)*rmsknt
            enddo
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columng%vv(jlev,jobs)=pvar(jlev,jobs)*rmsknt
            enddo
          enddo
        endif
      elseif(cdvar.eq.'GZ') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columnhr%gz(jlev,jobs)=pvar(jlev,jobs)*10.*rg
            enddo
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columng%gz(jlev,jobs)=pvar(jlev,jobs)*10.*rg
            enddo
          enddo
        endif
      elseif(cdvar.eq.'HU') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columnhr%hu(jlev,jobs)=log(max(pvar(jlev,jobs),rhumin))
            enddo
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columng%hu(jlev,jobs)=log(max(pvar(jlev,jobs),rhumin))
            enddo
          enddo
        endif
      elseif(cdvar.eq.'LQ') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columnhr%hu(jlev,jobs)=pvar(jlev,jobs)
            enddo
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columng%hu(jlev,jobs) = pvar(jlev,jobs)
            enddo
          enddo
        endif
      elseif(cdvar.eq.'ES') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columnhr%es(jlev,jobs)=pvar(jlev,jobs)
            enddo
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columng%es(jlev,jobs) = pvar(jlev,jobs)
            enddo
          enddo
        endif
      elseif(cdvar.eq.'TT') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columnhr%tt(jlev,jobs)=pvar(jlev,jobs) + tcdk
            enddo
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columng%tt(jlev,jobs) = pvar(jlev,jobs) + tcdk
            enddo
          enddo
        endif
      elseif(cdvar.eq.'TR') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columnhr%tr(jlev,jobs)=pvar(jlev,jobs)
            enddo
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            do jlev = 1, inlev
              columng%tr(jlev,jobs) = pvar(jlev,jobs)
            enddo
          enddo
        endif
      elseif(cdvar.eq.'P0') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            columnhr%ps(1,jobs)=pvar(1,jobs)*rmbtpa
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            columng%ps(1,jobs)=pvar(1,jobs)*rmbtpa
          enddo
        endif
      elseif(cdvar.eq.'TG') then
        if(cdmvo.eq.'HR') then
          do jobs = 1,nobtot
            columnhr%tg(1,jobs)=pvar(1,jobs)
          enddo
        elseif(cdmvo.eq.'BG') then
          do jobs = 1,nobtot
            columng%tg(1,jobs)=pvar(1,jobs)
          enddo
        endif
      endif
c
      return
      end
