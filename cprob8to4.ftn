      SUBROUTINE CPROB8TO4(lobsSpaceData)
#if defined (DOC) 
*
***s/r CPROB8TO4  -
*
*
*Author  : JM Belanger (CMDA/SMC)  Nov 2000
*Revision:
*
**    Purpose: Copy REAL*8 part of CMA back to REAL*4.
*              ROBDATA8(NCM_OMA) to ROBDATA(NCM_OMA) 
*              ROBDATA8(NCM_OMI) to ROBDATA(NCM_OMI) 
*              ROBDATA8(NCM_OMN) to ROBDATA(NCM_OMN) 
*
*Arguments
*
#endif
      use obsSpaceData_mod
      IMPLICIT NONE
*implicits
c
      type(struct_obs) :: lobsSpaceData
*
      INTEGER JDATA
*-----------------------------------------------------------------

      do JDATA=1,obs_numbody(lobsSpaceData)
        call obs_set_r4(lobsSpaceData,'OMA ',JDATA, real(obs_elem_r8(lobsSpaceData,'OMA ',JDATA)))
        call obs_set_r4(lobsSpaceData,'OMI ',JDATA, real(obs_elem_r8(lobsSpaceData,'OMI ',JDATA)))
        call obs_set_r4(lobsSpaceData,'OMN ',JDATA, real(obs_elem_r8(lobsSpaceData,'OMN ',JDATA)))
        call obs_set_r4(lobsSpaceData,'OMF ',JDATA, real(obs_elem_r8(lobsSpaceData,'OMF ',JDATA)))
        call obs_set_r4(lobsSpaceData,'VAR ',JDATA, real(obs_elem_r8(lobsSpaceData,'VAR ',JDATA)))
        call obs_set_r4(lobsSpaceData,'OER ',JDATA, real(obs_elem_r8(lobsSpaceData,'OER ',JDATA)))
        call obs_set_r4(lobsSpaceData,'PPP ',JDATA, real(obs_elem_r8(lobsSpaceData,'PPP ',JDATA)))
        call obs_set_r4(lobsSpaceData,'PRL ',JDATA, real(obs_elem_r8(lobsSpaceData,'PRL ',JDATA)))
      enddo

      RETURN
      END



