      SUBROUTINE CPROB8TO4
#if defined (DOC) 
*
***s/r CPROB8TO4  -
*
*
*Author  : JM Belanger (CMDA/SMC)  Nov 2000
*Revision:
*
**    Purpose: Copy REAL*8 part of CMA back to REAL*4.
*              ROBDATA8(NCM_OMA) to ROBDATA(NCM_OMA) 
*              ROBDATA8(NCM_OMI) to ROBDATA(NCM_OMI) 
*              ROBDATA8(NCM_OMN) to ROBDATA(NCM_OMN) 
*
*Arguments
*
#endif
      use obsSpaceData_mod
      IMPLICIT NONE
*implicits
#include "comdimo.cdk"


*
      INTEGER JDATA
*-----------------------------------------------------------------

      do JDATA=1,obs_num_obstotal(obsSpaceData)
        call obs_set(obsSpaceData,JDATA,val_OMA = real(obsSpaceData%ROBDATA8(NCM_OMA,JDATA)))
        call obs_set(obsSpaceData,JDATA,val_OMI = real(obsSpaceData%ROBDATA8(NCM_OMI,JDATA)))
        call obs_set(obsSpaceData,JDATA,val_OMN = real(obsSpaceData%ROBDATA8(NCM_OMN,JDATA)))
        call obs_set(obsSpaceData,JDATA,val_OMF = real(obsSpaceData%ROBDATA8(NCM_OMF,JDATA)))
        call obs_set(obsSpaceData,JDATA,val_VAR = real(obsSpaceData%ROBDATA8(NCM_VAR,JDATA)))
        call obs_set(obsSpaceData,JDATA,val_OER = real(obsSpaceData%ROBDATA8(NCM_OER,JDATA)))
        call obs_set(obsSpaceData,JDATA,val_PPP = real(obsSpaceData%ROBDATA8(NCM_PPP,JDATA)))
        call obs_set(obsSpaceData,JDATA,val_PRL = real(obsSpaceData%ROBDATA8(NCM_PRL,JDATA)))
      enddo

      RETURN
      END



